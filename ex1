#include <stdio.h>

int main(void) {
  int contador = 1;
  int fim = 3;
  float total = 0;

  while( contador <= fim ) {
    float nota;
    printf("Inserir %dª nota: ", contador);
    scanf ("%f", &nota);
    contador += 1;
    total += nota;
  }
  float media = total / 3;
  printf ("Média: %.2f", media);

  return 0;
}
