#include <stdio.h>

int main(void) {
  int contador = 1;
  int fim = 3;
  float total = 0;

  while( contador <= fim ) {
    float nota;
    printf("Nota %d: ", contador);
    scanf ("%f", &nota);
    contador = contador + 1;
    total = total + nota;
  }
  float media = total / 3;
  printf ("MÃ©dia: %.2f", media);

  return 0;
}
